# KAS 前端环境配置示例

# 应用基本信息
VITE_APP_TITLE=KAS系统
VITE_APP_VERSION=1.0.0

# API配置
VITE_API_BASE_URL=http://117.72.79.92:8080
VITE_API_TIMEOUT=30000

# WebSocket配置
VITE_WS_URL=ws://117.72.79.92:8080
VITE_WS_RECONNECT_INTERVAL=3000
VITE_WS_MAX_RECONNECT_ATTEMPTS=5

# 开发配置
VITE_DEV_MODE=false
VITE_DEBUG=false
VITE_MOCK_DATA=false

# 功能开关
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_AUTO_REFRESH=true

# UI配置
VITE_THEME=light
VITE_LANGUAGE=zh-CN

# 缓存配置
VITE_CACHE_ENABLED=true
VITE_CACHE_DURATION=300000

# 日志配置
VITE_LOG_LEVEL=info

# Electron配置
ELECTRON_WINDOW_WIDTH=1200
ELECTRON_WINDOW_HEIGHT=800
ELECTRON_DEV_SERVER_URL=http://localhost:5173
ELECTRON_SHOW_DEV_TOOLS=false